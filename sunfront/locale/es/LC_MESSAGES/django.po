#: templates/registration/activate.html:11
msgid "Your activation link has probably expired."
msgstr "Su enlace de activación posiblemente ha expirado."

#: templates/registration/activation_email.txt:3
msgid "Hi, dear new user."
msgstr "Hola, querido nuevo usuario:"

#: templates/registration/activation_email.txt:5
msgid "To acivate your account in"
msgstr "Para activar su cuenta en"

#: templates/registration/activation_email.txt:5
msgid "please, go to this URL"
msgstr "por favr, vaya a esta URL"

#: templates/registration/activation_email.txt:9
msgid "This URL expires in"
msgstr "Esta URL expira en"

#: templates/registration/activation_email.txt:9
